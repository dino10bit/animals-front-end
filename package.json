    "downshift": "^6.1.0",
    "express": "4.19.2",
    "formik": "2.2.6",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "history": "5.0.0",
    "invariant": "2.2.4",
    "js-cookie": "2.2.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-lines-ellipsis": "0.14.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1",
